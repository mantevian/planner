<plan w="7" h="7">
	<defs>
		<color name="wall" value="#444444" />
		<color name="contour" value="#cccccc" />

		<template name="couch" />
		<template name="table" />
		<template name="wash" />
	</defs>

	<axes>
		<axis id="A" type="vertical" />
		<axis id="B" type="vertical" offset="1" />
		<axis id="C" type="vertical" offset="1" />
		<axis id="D" type="vertical" offset="1" />
		<axis id="E" type="vertical" offset="1" />

		<axis id="a" type="horizontal" />
		<axis id="b" type="horizontal" offset="1" />
		<axis id="c" type="horizontal" offset="1" />
		<axis id="d" type="horizontal" offset="1" />
		<axis id="e" type="horizontal" offset="1" />
	</axes>

	<flat>
		<room>
			<walls>
				<wall from="A a" to="B a" width="0.1" />

				<wall to="B b" width="0.1" />

				<wall to="D b" width="0.1">
					<window offset="0.75" width="0.5" />
				</wall>

				<wall to="E c" width="0.1">
					<door offset="0.45" width="0.5" />
				</wall>

				<wall to="E e" width="0.1">
					<window offset="0.75" width="0.5" />
				</wall>

				<wall to="C e" width="0.1" />

				<wall to="B d" width="0.1" />

				<wall to="A d" width="0.1" />

				<wall to="A a" width="0.1" />
			</walls>
		</room>
	</flat>
</plan>