<plan w="7" h="7" mode="debug">
	<defs>
		<color name="wall" value="#444444" />
		<color name="contour" value="#cccccc" />

		<template name="couch" />
		<template name="table" />
		<template name="wash" />
	</defs>

	<axes>
		<axis id="A" type="vertical" />
		<axis id="B" type="vertical" offset="1" />
		<axis id="C" type="vertical" offset="1" />
		<axis id="D" type="vertical" offset="1" />
		<axis id="E" type="vertical" offset="1" />

		<axis id="a" type="horizontal" />
		<axis id="b" type="horizontal" offset="1" />
		<axis id="c" type="horizontal" offset="1" />
		<axis id="d" type="horizontal" offset="1" />
		<axis id="e" type="horizontal" offset="1" />
	</axes>

	<flat>
		<features>
			<window wall="A a, C a" offset="0.75" width="0.5" />
			<door wall="A c, A a" offset="0.75" width="0.5" />
		</features>

		<room>
			<walls>
				<wall from="A a" to="C a" thickness="0.2" />
				<wall to="C c" thickness="0.1" />
				<wall to="A c" thickness="0.1" />
				<wall thickness="0.1" />
			</walls>
		</room>

		<room>
			<walls>
				<wall from="A c" to="D c" thickness="0.1" />
				<wall to="D e" thickness="0.1" />
				<wall to="A e" thickness="0.1" />
				<wall thickness="0.1" />
			</walls>
		</room>
	</flat>
</plan>